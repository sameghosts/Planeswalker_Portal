<% if (alerts.error){ %>
  <div class="alert-container error">
      <% alerts.error.forEach(msg =>{ %>
        <p class="alert">
          <%= msg %>
        </p>
    </div>
  <% }) %>
<% } %>

<% if (alerts.success){ %>
  <div class="alert-container sucess">
      <% alerts.success.forEach(msg =>{ %>
        <p class="alert">
          <%= msg %>
        </p>
    </div>
  <% }) %>
<% } %>
